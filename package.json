{
  "name": "pettrack",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec \"node --max-old-space-size=4096 -r ts-node/register src/index.ts\"",
    "build": "tsc",
    "start": "node --max-old-space-size=4096 dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:reset": "prisma migrate reset --force",
    "db:setup": "npm run prisma:generate && npm run prisma:migrate && npm run prisma:seed",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "pet-tracking",
    "qr-codes",
    "nodejs",
    "express",
    "prisma",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "description": "Pet tracking application with QR code integration",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@types/nodemailer": "^6.4.17",
    "@types/twilio": "^3.19.2",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.4.0",
    "google-auth-library": "^10.2.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-apple-signin": "^1.2.2",
    "nodemailer": "^7.0.5",
    "qrcode": "^1.5.4",
    "razorpay": "^2.9.6",
    "twilio": "^5.8.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
